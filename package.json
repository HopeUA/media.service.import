{
  "name": "media-service-import",
  "description": "Media library import for hope.ua",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Sergey Saodovi",
    "email": "sergey@hope.ua"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HopeUA/media.service.import"
  },
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "mocha",
    "lint": "eslint ./bin ./src"
  },
  "betterScripts": {
    "start": {
      "command": "node -r babel-register -r babel-polyfill bin/app.js",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "dev": {
      "command": "node -r babel-register -r babel-polyfill bin/app.js",
      "env": {
        "NODE_PATH": "./src",
        "DEBUG": "app"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "better-npm-run": "0.0.5",
    "chalk": "^1.1.1",
    "config": "^1.17.1",
    "moment": "^2.11.0",
    "mysql": "^2.9.0",
    "node-fetch": "^1.3.3",
    "string": "^3.3.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "chai": "^3.4.1",
    "debug": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-hope": "^1.0.2",
    "eslint-plugin-react": "^3.13.1",
    "mocha": "^2.3.4"
  }
}
